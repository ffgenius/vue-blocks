{
    "name": "vueblocks",
    "type": "module",
    "version": "0.0.0",
    "private": true,
    "description": "The ultimate front-end building block library specifically designed for Vue",
    "author": "OldDriver(lonewolfyx) <https://github.com/lonewolfyx>",
    "license": "MIT",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxi dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "generator:registry.json": "tsx ./scripts/generator.registry.json.ts",
        "register:build": "shadcn-vue build --output ./public/r",
        "generator:menus": "tsx ./scripts/menus.ts",
        "build:shadcn:data": "npm run generator:registry.json && npm run register:build && npm run generator:menus",
        "lint:fix": "eslint --fix"
    },
    "homepage": "https://github.com/vue-blocks/vue-blocks#readme",
    "bugs": {
        "url": "https://github.com/vue-blocks/vue-blocks/issues"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:vue-blocks/vue-blocks.git"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "@vueuse/core": "^13.9.0",
        "@vueuse/nuxt": "^13.5.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-vue-next": "^0.525.0",
        "nuxt": "^4.0.0",
        "nuxt-shiki": "^0.3.1",
        "prismjs": "^1.30.0",
        "radash": "^12.1.1",
        "reka-ui": "^2.5.0",
        "shadcn-nuxt": "^2.2.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "tw-animate-css": "^1.3.5",
        "vue": "^3.5.17",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@iconify/vue": "^5.0.0",
        "@nuxt/eslint": "^1.6.0",
        "@nuxt/icon": "^1.15.0",
        "@shikijs/transformers": "^3.12.2",
        "@types/node": "^24.0.14",
        "eslint": "^9.31.0",
        "glob": "^11.0.3",
        "shadcn-vue": "^2.2.0",
        "shiki": "^3.12.2",
        "tsx": "^4.20.5",
        "typescript": "^5.8.3"
    }
}

      "content": "<template>\n    <section class=\"py-32\">\n        <div class=\"container\">\n            <div class=\"flex flex-col items-center justify-center gap-4 text-center\">\n                <h4 class=\"text-3xl font-medium lg:text-4xl\">\n                    Powering the world's best product teams\n                </h4>\n                <p class=\"text-xl text-muted-foreground lg:-mt-1\">\n                    From next-gen startups to established enterprises\n                </p>\n            </div>\n\n            <div class=\"relative my-20 overflow-hidden\">\n                <div class=\"flex w-full\">\n                    <div class=\"flex shrink-0 animate-marquee items-center gap-4\">\n                        <a\n                            v-for=\"logo in DATA\"\n                            :key=\"logo.key\"\n                            class=\"p-4\"\n                            :href=\"logo.href\"\n                            target=\"_blank\"\n                        >\n                            <img\n                                :alt=\"logo.alt\"\n                                class=\"object-contain transition-opacity hover:opacity-70\"\n                                :height=\"logo.height\"\n                                :src=\"logo.src\"\n                                :width=\"logo.width\"\n                            >\n                        </a>\n                    </div>\n                    <div class=\"flex shrink-0 animate-marquee items-center gap-4\">\n                        <a\n                            v-for=\"logo in DATA\"\n                            :key=\"logo.key\"\n                            class=\"p-4\"\n                            :href=\"logo.href\"\n                            target=\"_blank\"\n                        >\n                            <img\n                                :alt=\"logo.alt\"\n                                class=\"object-contain transition-opacity hover:opacity-70\"\n        